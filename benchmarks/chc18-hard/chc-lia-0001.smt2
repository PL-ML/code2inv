(set-logic ALL)
(declare-rel itp (Int Int))
(declare-rel simple!!query ())
(declare-var A Int)
(declare-var B Int)
(declare-var C Int)
(declare-var D Int)
(rule (=> (and (not (<= A B)) (= B 0)) (itp B A)))
(rule (=> (and (itp D C) (= A (+ 2 C)) (= B (+ 1 D))) (itp B A)))
(rule (=> (and (itp A B) (<= B 2000) (not (<= A 1000))) simple!!query))
(query simple!!query)
