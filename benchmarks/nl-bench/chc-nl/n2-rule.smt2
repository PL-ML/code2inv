(set-logic ALL)
(declare-rel itp (Int Int))
(declare-rel simple!!query ())
(declare-var A Int)
(declare-var B Int)
(declare-var C Int)
(declare-var D Int)
(rule (=> (and (= A 1) (= B 0)) (itp A B)))
(rule (=> (and (itp A B) (= D (+ B 1)) (= C (+ A D))) (itp C D)))
(rule (=> (and (itp B A) (> (* A A) (+ B B))) simple!!query))
(query simple!!query)
