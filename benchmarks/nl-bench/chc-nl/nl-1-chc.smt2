(set-logic ALL)
(declare-rel itp (Int Int))
(declare-rel simple!!query ())
(declare-var A Int)
(declare-var B Int)
(declare-var C Int)
(declare-var D Int)
(rule (=> (and (= A 0) (= B 0)) (itp A B)))
(rule (=> (and (itp A B) (= D (+ B 1)) (= C (* D D))) (itp C D)))
(rule (=> (and (itp A B) (not (= A (* B B)))) simple!!query))
(query simple!!query)
