(set-logic ALL)
(declare-rel itp (Int Int))
(declare-rel simple!!query ())
(declare-var X Int)
(declare-var Y Int)
(declare-var X1 Int)
(declare-var Y1 Int)
(rule (=> (and (>= X 0) (<= X 10) (= Y (* X X))) (itp X Y)))
(rule (=> (and (itp X Y) (<= (* X X) 1000) (= Y1 (+ Y 1)) (= X1 (+ X 1))) (itp X1 Y1)))
(rule (=> (and (itp X Y) (not (<= (* X X) 1000)) (not (<= Y 1000))) simple!!query))
(query simple!!query)
